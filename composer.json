{
    "name": "jp3cki/haraikomi",
    "description": "",
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">= 8.4",
        "php-64bit": "*",
        "php-ipv6": "*",
        "ext-ctype": "*",
        "ext-intl": "*",
        "ext-mbstring": "*",
        "ext-pdo": "*",
        "ext-pdo_sqlite": "*",
        "ext-sqlite3": "*",
        "masterminds/html5": "^2.10.0",
        "paragonie/constant_time_encoding": "^3.1.3",
        "symfony/css-selector": "^6.4.24",
        "symfony/yaml": "^6.4.30",
        "tecnickcom/tcpdf": "^6.10.1",
        "twbs/bootstrap-icons": "^1.13.1",
        "yiisoft/yii2": "^2.0.54",
        "yiisoft/yii2-bootstrap5": "^2.0.51",
        "yiisoft/yii2-httpclient": "^2.0.16"
    },
    "require-dev": {
        "codeception/codeception": "^5.3.4",
        "codeception/module-asserts": "^3.2.1",
        "codeception/module-filesystem": "^3.0.2",
        "codeception/module-phpbrowser": "^3.0.2",
        "codeception/module-rest": "^3.4.3",
        "codeception/module-yii2": "^1.1.12",
        "codeception/specify": "^2.0.0",
        "codeception/verify": "^3.0.0.",
        "dealerdirect/phpcodesniffer-composer-installer": "^1.2.0",
        "ergebnis/composer-normalize": "^2.50",
        "jp3cki/coding-standard": "^2.0.0",
        "proget-hq/phpstan-yii2": "^0.8.0",
        "squizlabs/php_codesniffer": "^3.13.5",
        "symfony/browser-kit": "^6.4.32",
        "yiisoft/yii2-debug": "^2.1.27",
        "yiisoft/yii2-gii": "^2.2.7"
    },
    "replace": {
        "bower-asset/bootstrap": "^5.0",
        "bower-asset/inputmask": "*",
        "bower-asset/jquery": "*",
        "bower-asset/punycode": "*",
        "bower-asset/yii2-pjax": "*",
        "npm-asset/bootstrap": "*",
        "paragonie/random_compat": "*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-intl-grapheme": "*",
        "symfony/polyfill-intl-normalizer": "*",
        "symfony/polyfill-mbstring": "*"
    },
    "minimum-stability": "stable",
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "ergebnis/composer-normalize": true,
            "yiisoft/yii2-composer": true
        },
        "platform": {
            "php": "8.4.3"
        },
        "process-timeout": 1800,
        "sort-packages": true
    },
    "scripts": {
        "post-install-cmd": [
            "[ $COMPOSER_DEV_MODE -eq 0 ] || ./vendor/bin/codecept build"
        ],
        "post-update-cmd": [
            "[ $COMPOSER_DEV_MODE -eq 0 ] || ./vendor/bin/codecept build"
        ]
    }
}
