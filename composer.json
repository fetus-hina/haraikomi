{
    "name": "jp3cki/haraikomi",
    "description": "",
    "type": "project",
    "license": "MIT",
    "minimum-stability": "stable",
    "require": {
        "php": ">= 8.0",
        "masterminds/html5": "^2.7.5",
        "symfony/css-selector": "^5.3.4",
        "symfony/polyfill-php80": "^1.23.1",
        "tecnickcom/tcpdf": "^6.4.2",
        "yiisoft/yii2": "~2.0.43",
        "yiisoft/yii2-bootstrap5": "~2.0.2",
        "yiisoft/yii2-httpclient": "~2.0.13"
    },
    "require-dev": {
        "codeception/codeception": "^4.1.22",
        "codeception/module-asserts": "^1.3.1",
        "codeception/module-filesystem": "^2.0.0",
        "codeception/module-phpbrowser": "^1.0.2",
        "codeception/module-rest": "^1.3.1",
        "codeception/module-yii2": "^1.1.3",
        "codeception/specify": "^1.4.0",
        "codeception/verify": "^2.1.1",
        "dealerdirect/phpcodesniffer-composer-installer": "^0.7.1",
        "phpcompatibility/php-compatibility": "^9.3.5",
        "phpstan/phpstan": "^0.12.99",
        "proget-hq/phpstan-yii2": "^0.7.6",
        "sirbrillig/phpcs-import-detection": "^1.3.2",
        "sirbrillig/phpcs-variable-analysis": "^2.11.2",
        "squizlabs/php_codesniffer": "^3.6.0",
        "symfony/browser-kit": "^5.3.4",
        "yiisoft/yii2-debug": "~2.1.18",
        "yiisoft/yii2-gii": "~2.2.3"
    },
    "replace": {
        "bower-asset/bootstrap": "5.*",
        "bower-asset/inputmask": "~3.3.11",
        "bower-asset/jquery": "*",
        "bower-asset/punycode": "1.3.*",
        "bower-asset/yii2-pjax": "*",
        "npm-asset/bootstrap": "5.*"
    },
    "config": {
        "process-timeout": 1800,
        "platform": {
            "php": "8.0.10"
        },
        "sort-packages": true
    },
    "scripts": {
        "post-install-cmd": [
            "[ $COMPOSER_DEV_MODE -eq 0 ] || ./vendor/bin/codecept build"
        ],
        "post-update-cmd": [
            "[ $COMPOSER_DEV_MODE -eq 0 ] || ./vendor/bin/codecept build"
        ]
    }
}
