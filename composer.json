{
    "name": "jp3cki/haraikomi",
    "description": "",
    "type": "project",
    "license": "MIT",
    "minimum-stability": "stable",
    "require": {
        "php": ">= 8.1",
        "masterminds/html5": "^2.8.1",
        "myclabs/php-enum": "^1.8.4",
        "paragonie/constant_time_encoding": "^2.6.3",
        "symfony/css-selector": "*",
        "symfony/polyfill-php80": "*",
        "symfony/yaml": "*",
        "tecnickcom/tcpdf": "^6.6.5",
        "twbs/bootstrap-icons": "^1.11.1",
        "yiisoft/yii2": "*",
        "yiisoft/yii2-bootstrap5": "*",
        "yiisoft/yii2-httpclient": "*"
    },
    "require-dev": {
        "codeception/codeception": "*",
        "codeception/module-asserts": "*",
        "codeception/module-filesystem": "*",
        "codeception/module-phpbrowser": "*",
        "codeception/module-rest": "*",
        "codeception/module-yii2": "*",
        "codeception/specify": "*",
        "codeception/verify": "*",
        "dealerdirect/phpcodesniffer-composer-installer": "*",
        "jp3cki/coding-standard": "*",
        "proget-hq/phpstan-yii2": "*",
        "squizlabs/php_codesniffer": "*",
        "symfony/browser-kit": "*",
        "yiisoft/yii2-debug": "*",
        "yiisoft/yii2-gii": "*"
    },
    "replace": {
        "bower-asset/bootstrap": "5.*",
        "bower-asset/inputmask": "~3.3.11",
        "bower-asset/jquery": "*",
        "bower-asset/punycode": "1.3.*",
        "bower-asset/yii2-pjax": "*",
        "npm-asset/bootstrap": "5.*"
    },
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "yiisoft/yii2-composer": true
        },
        "platform": {
            "php": "8.1.23"
        },
        "process-timeout": 1800,
        "sort-packages": true
    },
    "scripts": {
        "post-install-cmd": [
            "[ $COMPOSER_DEV_MODE -eq 0 ] || ./vendor/bin/codecept build"
        ],
        "post-update-cmd": [
            "[ $COMPOSER_DEV_MODE -eq 0 ] || ./vendor/bin/codecept build"
        ]
    }
}
