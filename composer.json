{
    "name": "jp3cki/haraikomi",
    "description": "",
    "type": "project",
    "license": "MIT",
    "minimum-stability": "stable",
    "require": {
        "php": ">= 8.1",
        "masterminds/html5": "^2.8.1",
        "myclabs/php-enum": "^1.8.4",
        "paragonie/constant_time_encoding": "^2.6.3",
        "symfony/css-selector": "^6.3.2",
        "symfony/yaml": "^6.3.3",
        "tecnickcom/tcpdf": "^6.6.5",
        "twbs/bootstrap-icons": "^1.11.1",
        "yiisoft/yii2": "^2.0.49",
        "yiisoft/yii2-bootstrap5": "^2.0.4",
        "yiisoft/yii2-httpclient": "^2.0.15"
    },
    "require-dev": {
        "codeception/codeception": "^5.0.11",
        "codeception/module-asserts": "^3.0.0",
        "codeception/module-filesystem": "^3.0.0",
        "codeception/module-phpbrowser": "^3.0.0",
        "codeception/module-rest": "^3.3.2",
        "codeception/module-yii2": "^1.1.9",
        "codeception/specify": "^2.0.0",
        "codeception/verify": "^3.0.0.",
        "dealerdirect/phpcodesniffer-composer-installer": "^1.0.0",
        "jp3cki/coding-standard": "^0.1.3",
        "proget-hq/phpstan-yii2": "^0.8.0",
        "squizlabs/php_codesniffer": "^3.7.2",
        "symfony/browser-kit": "^6.3.2",
        "yiisoft/yii2-debug": "^2.1.25",
        "yiisoft/yii2-gii": "^2.2.6"
    },
    "replace": {
        "bower-asset/bootstrap": "5.*",
        "bower-asset/inputmask": "~3.3.11",
        "bower-asset/jquery": "*",
        "bower-asset/punycode": "1.3.*",
        "bower-asset/yii2-pjax": "*",
        "npm-asset/bootstrap": "5.*"
    },
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "yiisoft/yii2-composer": true
        },
        "platform": {
            "php": "8.1.23"
        },
        "process-timeout": 1800,
        "sort-packages": true
    },
    "scripts": {
        "post-install-cmd": [
            "[ $COMPOSER_DEV_MODE -eq 0 ] || ./vendor/bin/codecept build"
        ],
        "post-update-cmd": [
            "[ $COMPOSER_DEV_MODE -eq 0 ] || ./vendor/bin/codecept build"
        ]
    }
}
